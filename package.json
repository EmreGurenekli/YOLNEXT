{
  "name": "yolnet-kargo-platform",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:frontend": "vite",
    "dev:backend": "cd backend && node postgres-backend.js",
    "dev:all": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "build": "vite build",
    "build:frontend": "tsc && vite build",
    "build:backend": "cd backend && npm run build",
    "build:all": "npm run build:frontend && npm run build:backend",
    "preview": "vite preview",
    "start": "npm run dev:all",
    "start:frontend": "npm run dev:frontend",
    "start:backend": "npm run dev:backend",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:browser": "node tests/e2e/browser-workflow-test.cjs",
    "test:workflow": "node tests/e2e/browser-workflow-test.cjs",
    "test:mcp-fallback": "node tests/root-tests/test-playwright-direct.js",
    "test:mcp-wrapper": "node tools/run-mcp-playwright-test.js",
    "test:comprehensive-flow": "node tests/comprehensive-user-flow-test.js",
    "test:search": "node tests/search-engine-test.js",
    "test:advanced": "node tests/advanced-comprehensive-test.js",
    "test:all-advanced": "node tests/advanced-comprehensive-test.js && node tests/comprehensive-user-flow-test.js && node tests/search-engine-test.js",
    "test:comprehensive": "node tests/e2e/comprehensive-all-panels-test.cjs",
    "test:real-data": "node tests/e2e/real-data-flow-test.cjs",
    "test:verify-data": "node tests/e2e/verify-real-data.cjs",
    "test:all": "npm run test && npm run test:e2e",
    "install:all": "npm install && cd backend && npm install",
    "clean": "rimraf dist node_modules/.vite",
    "clean:all": "npm run clean && cd backend && npm run clean",
    "setup": "npm run install:all && npm run build:all",
    "deploy": "bash scripts/deploy-production.sh",
    "migrate:prod": "cd backend && node scripts/migrate-production.js",
    "load-test": "node tools/load-test.cjs",
    "security-audit": "node tools/security-audit.cjs",
    "health-check": "node tools/health-check.cjs",
    "validate:prod": "node scripts/validate-production.js",
    "deploy:prod": "bash scripts/deploy-production.sh",
    "start:prod": "node scripts/start-production.js",
    "fix:all": "node scripts/fix-all-issues.js",
    "test:live": "node --max-old-space-size=2048 tools/comprehensive-live-test.cjs",
    "test:live:safe": "node --max-old-space-size=1536 tools/comprehensive-live-test-safe.cjs",
    "test:live:ultra": "node --max-old-space-size=1536 tools/comprehensive-live-test-safe.cjs",
    "test:protection-check": "node tools/project-protection-check.cjs",
    "setup-monitoring": "node backend/scripts/setup-monitoring.js",
    "deploy:docker": "docker-compose up -d --build",
    "deploy:stop": "docker-compose down",
    "deploy:logs": "docker-compose logs -f"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.2",
    "@tanstack/react-query": "^5.90.5",
    "axios": "^1.6.2",
    "bcrypt": "^5.1.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "express": "^5.1.0",
    "helmet": "^7.2.0",
    "lucide-react": "^0.294.0",
    "pg": "^8.16.3",
    "puppeteer": "^24.26.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.3.0",
    "react-hook-form": "^7.48.2",
    "react-hot-toast": "^2.4.1",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.20.1",
    "sequelize": "^6.37.7",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^2.0.0",
    "tailwindcss": "^3.3.6",
    "tailwindcss-animate": "^1.0.7",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/plugin-transform-runtime": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@playwright/test": "^1.40.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/d3-array": "^3.2.2",
    "@types/d3-interpolate": "^3.0.4",
    "@types/d3-scale": "^4.0.9",
    "@types/d3-shape": "^3.1.7",
    "@types/d3-time": "^3.0.4",
    "@types/jest": "^30.0.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/ui": "^1.0.0",
    "autoprefixer": "^10.4.16",
    "axe-core": "^4.8.2",
    "babel-jest": "^29.7.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.53.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^27.0.0",
    "playwright": "^1.56.1",
    "postcss": "^8.4.32",
    "prettier": "^3.6.2",
    "rimraf": "^5.0.5",
    "supertest": "^7.1.4",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "vite-plugin-pwa": "^0.17.4",
    "vitest": "^1.6.1"
  }
}
