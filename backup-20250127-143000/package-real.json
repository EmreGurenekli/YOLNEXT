{
  "name": "yolnet-real-marketplace",
  "version": "2.0.0",
  "description": "YolNet Gerçek Kargo Pazaryeri Platformu",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:frontend": "vite",
    "dev:backend": "node backend/real-server.js",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "vite build",
    "build:backend": "echo 'Backend build completed'",
    "start": "node backend/real-server.js",
    "start:prod": "NODE_ENV=production node backend/real-server.js",
    "init:db": "node backend/scripts/init-real-database.js",
    "reset:db": "rm -f backend/database/yolnet_real.db && npm run init:db",
    "test": "echo 'Tests will be implemented'",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "preview": "vite preview",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.1",
    "react-helmet-async": "^1.3.0",
    "lucide-react": "^0.263.1",
    "framer-motion": "^10.16.4",
    "axios": "^1.6.0",
    "socket.io-client": "^4.7.4",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "sqlite3": "^5.1.6",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "dotenv": "^16.3.1",
    "express-validator": "^7.0.1",
    "joi": "^17.11.0",
    "multer": "^1.4.5-lts.1",
    "uuid": "^9.0.1",
    "socket.io": "^4.7.4"
  },
  "devDependencies": {
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/node": "^20.8.10",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.1.1",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "eslint": "^8.53.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.2.2",
    "vite": "^4.5.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "keywords": [
    "kargo",
    "pazaryeri",
    "lojistik",
    "nakliye",
    "taşıma",
    "marketplace",
    "logistics",
    "shipping"
  ],
  "author": "YolNet Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yolnet/real-marketplace"
  },
  "bugs": {
    "url": "https://github.com/yolnet/real-marketplace/issues"
  },
  "homepage": "https://yolnet.com"
}




